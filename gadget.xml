<?xml version="1.0" encoding="utf-8" ?>
<gadget>
  <name>HelpDesk</name>
  <namespace>de.wwerther.gadgets.helpdesk</namespace>
  <version>0.7</version>
  <author name="Walter Werther">
	  <info url="http://www.wwerther.de/" text="About this plugin"/> 
	  <logo height="50" width="100" src="author.png"/>
  </author>
  <copyright>&#169; 2011 Walter Werther</copyright>
  <description><!--_locComment_text="{Locked}"-->A small toolbox containing functions that are commonly used in the HelpDesk
 - Fast access to Remote-Assistance and Remote-Console.
 - Fast access to Ping, Traceroute and Putty </description>
  <icons>
    <icon height="100" width="125" src="logo.png"/>
  </icons>
  <hosts>
      <host name="sidebar">
      <autoscaleDPI>true</autoscaleDPI>
      <base type="HTML" apiVersion="1.0.0" src="main.html" />
      <permissions>full</permissions>
      <platform minPlatformVersion="0.3" />
      <defaultImage src="drag.png"/>
    </host>
  </hosts>
</gadget>
